sync {
    default.rsyncssh,
    source="<%= @options[:source] %>",
    host="<%= @options[:host] %>",
    targetdir="<%= @options[:target] %>",
    delay=0,
    excludeFrom="/etc/lsyncd/exclude.conf",
    rsync = {
        archive = true,
        compress = true,
        owner = true,
        perms = true,
        group = true
    }
}
